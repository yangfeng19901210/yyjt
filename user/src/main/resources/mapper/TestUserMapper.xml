<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yy.user.mapper.TestUserMapper">

    <resultMap id="BaseResultMap" type="com.yy.user.entity.TestUser">
            <id property="uuid" column="uuid" jdbcType="VARCHAR"/>
            <result property="userId" column="user_id" jdbcType="INTEGER"/>
            <result property="userInfo" column="user_info" jdbcType="OTHER" typeHandler="com.yy.user.config.mybatis.JsonbTypeHandler"/>
    </resultMap>

    <sql id="Base_Column_List">
        uuid,user_id,user_info
    </sql>
</mapper>
